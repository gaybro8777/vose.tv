{
  "name": "vose",
  "version": "1.0.0",
  "description": "Reddit Video App",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "nodemon -r esm -r dotenv/config src/main.server.development.js & parcel watch src/main.client.development.js -d .public",
    "production": "rm -r .public; NODE_ENV=production parcel build src/index.html --experimental-scope-hoisting --no-source-maps -d .public && NODE_ENV=production parcel build src/main.server.js --no-source-maps --target node -d .server",
    "update-stats": "npx browserslist-ga",
    "lint": "eslint '**/*.js'",
    "deploy": "yarn update-stats; NODE_ENV=production yarn production && rsync -avzrt --update .server/ vose:/home/vose/app && rsync -avzrt --update .public/ vultr:/var/www/vose.tv/html"
  },
  "author": "Simon Laroche <hi@simon.lc> (https://simon.lc/)",
  "license": "MIT",
  "dependencies": {
    "@simonlc/menu-button": "0.1.9",
    "@simonlc/unstated": "2.1.3",
    "@simonlc/youtube-player": "6.0.0",
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "isomorphic-fetch": "2.2.1",
    "nodalytics": "1.1.0",
    "obj-str": "1.0.1",
    "react": "16.5.2",
    "react-dom": "16.5.2"
  },
  "devDependencies": {
    "@babel/core": "7.1.0",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "@simonlc/parcel-plugin-bundle-manifest": "0.1.3",
    "autoprefixer": "9.1.5",
    "babel-eslint": "9.0.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.15",
    "eslint": "5.6.0",
    "eslint-config-prettier": "3.0.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-react": "7.11.1",
    "esm": "3.0.84",
    "ignore-styles": "5.0.1",
    "nodemon": "1.18.4",
    "parcel-bundler": "1.10.0-beta.1",
    "parcel-plugin-bundle-visualiser": "1.2.0",
    "parcel-plugin-eslint": "1.0.4",
    "parcel-plugin-static-files-copy": "1.2.1",
    "postcss-nested": "4.1.0",
    "postcss-normalize": "7.0.1",
    "postcss-reporter": "6.0.0",
    "prettier": "1.14.3",
    "prop-types": "15.6.2",
    "react-hot-loader": "4.3.8"
  }
}
