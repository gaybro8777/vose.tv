{
  "name": "vose",
  "version": "1.0.0",
  "description": "Reddit Video App",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "nodemon src/main.server.development.js & parcel watch src/main.client.js -d .public",
    "production": "NODE_ENV=production rm -r .public; parcel build src/index.html --no-source-maps -d .public && parcel build src/main.server.js --no-source-maps --target node -d .server",
    "update-stats": "npx browserslist-ga",
    "lint": "eslint '**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "staticPath": "./src/assets",
  "author": "Simon Laroche <hi@simon.lc> (https://simon.lc/)",
  "license": "MIT",
  "dependencies": {
    "@reach/menu-button": "0.1.7",
    "@simonlc/youtube-player": "4.0.2",
    "babel-eslint": "9.0.0",
    "compression": "1.7.2",
    "dotenv": "6.0.0",
    "eslint": "5.6.0",
    "eslint-config-prettier": "3.0.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-react": "7.11.1",
    "esm": "3.0.66",
    "express": "4.16.3",
    "isomorphic-fetch": "2.2.1",
    "nodalytics": "1.1.0",
    "obj-str": "1.0.0",
    "prettier": "1.14.2",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "unstated": "2.1.1"
  },
  "devDependencies": {
    "@simonlc/parcel-plugin-bundle-manifest": "0.1.3",
    "autoprefixer": "8.6.5",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "ignore-styles": "5.0.1",
    "nodemon": "1.18.1",
    "parcel-bundler": "1.9.5",
    "parcel-plugin-bundle-visualiser": "1.1.2",
    "postcss-nested": "3.0.0",
    "postcss-normalize": "6.0.0",
    "postcss-reporter": "5.0.0",
    "prop-types": "15.6.2",
    "react-hot-loader": "4.3.3"
  }
